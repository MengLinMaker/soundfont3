{
  "name": "@menglinmaker/soundfont3",
  "version": "0.0.2",
  "description": "A SoundFont3 parser for Node.js and web browsers",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "soundfont3",
    "soundfont",
    "sf3",
    "midi",
    "synthesizer"
  ],
  "author": "Meng Lin <menglinmaker@gmail.com>",
  "repository": "https://github.com/musidi-org/soundfont3.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '**/*.ts' && tsc",
    "test": "jest --coverage",
    "prettier": "prettier --write --config ./.prettierrc --list-different '**/*.ts'",
    "build": "rimraf dist && rollup -c && pnpm run prettier && pnpm link .",
    "docs:deploy": "vitepress build docs && gh-pages -d docs/.vitepress/dist",
    "docs:start": "vitepress build docs && vitepress preview docs",
    "docs:dev": "vitepress dev docs",
    "release": "pnpm run build && changeset publish"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@eslint/js": "^9.5.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.60",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.23.0",
    "eslint": "^9.5.0",
    "gh-pages": "^2.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^8.2.1",
    "node-web-audio-api": "^0.21.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "riff-file": "^1.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "smplr": "^0.15.1",
    "soundfont3": "file:.",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitepress": "^1.2.3",
    "vue": "^3.4.31"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --config ./.prettierrc --list-different '**/*.ts'",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "pnpm run lint && pnpm run test && lint-staged"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "audio-decode": "^2.2.0"
  }
}
